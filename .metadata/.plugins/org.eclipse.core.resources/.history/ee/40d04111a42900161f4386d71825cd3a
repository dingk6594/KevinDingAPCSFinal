package OneonOneBattle;

import java.awt.Image;

public class Player extends Moveable {
	
	private Image img;
	private int xPos;
	private int yPos;
	private int height;
	private int width;
	

	public Player (final int xPos,final int yPos, final int height, final int width, final Image img){
		
		this.xPos = xPos;
		this.yPos = yPos;
		this.height = height;
		this.width = width;
		this.img = img;
	}}


	