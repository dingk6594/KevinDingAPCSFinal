package OneonOneBattle;

import java.awt.Graphics;
import java.awt.Image;

public class MoveableThing extends Moveable {
	
	private int deltaX;
	
	private Image img;
	private int xPos;
	private int yPos;
	private int height;
	private int width;
	private int health;
	

	public MoveableThing (final int xPos,final int yPos, final int width, final int height, final Image img)
	{
		
		this.xPos = xPos;
		this.yPos = yPos;
		this.height = height;
		this.width = width;
		this.img = img;
		
	
	}
	


	@Override
	void draw(Graphics g) 
	{
		g.drawImage(img, xPos, yPos, height, width, null);
	}

		
		

	@Override
	void update() {
		
	}
	
	@Override
	Image getImage(String img) {
		// TODO Auto-generated method stub
		return null;
	}

	public int getDeltaX() {
		return deltaX;
	}



	public void setDeltaX(int deltaX) {
		this.deltaX = deltaX;
	}



	
}
